  "dependencies": {
    "gh-pages": "^2.0.1",
    "i18next": "^11.9.0",
    "lodash": "^4.17.11",
    "react": "^16.5.2",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "test": "jest --color --coverage --verbose",
    "eject": "react-scripts eject"
